



<div class="container">
  <div class="row justify-content-md-center">
    <div class="col-6 ">
      <h1>New bookmark</h1>
      <%= simple_form_for([@list, @bookmark]) do |f| %>
      <%= f.input :comment %>
      <%= f.association :movie %>
      <%= f.submit %>
      <% end %>
    </div>
  </div>
</div>
